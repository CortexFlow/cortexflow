{
  "name": "cortexflow",
  "homepage": "https://www.cortexflow.org/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "concurrently": "^9.2.0",
    "cortexflow": "file:",
    "glightbox": "^3.3.0",
    "lucide-react": "^0.525.0",
    "matchmedia-polyfill": "^0.3.2",
    "postcss-cli": "^11.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^3.4.17",
    "typed.js": "^2.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run tailwind-dev\" \"npm run start-react\"",
    "start-react": "react-scripts start",
    "build-prod": "npm run tailwind-prod && npm run react-scripts build",
    "tailwind-dev": "tailwindcss -i ./src/index.css -o ./public/style.css --watch",
    "tailwind-prod": "tailwindcss -i ./src/index.css -o ./public/style.css --minify",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "GENERATE_SOURCEMAP=false npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.1.1",
    "postcss": "^8.5.6"
  }
}
